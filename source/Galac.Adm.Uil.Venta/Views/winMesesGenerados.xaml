<Window x:Class="Galac.Adm.Uil.Venta.winMesesGenerados"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Galac.Adm.Uil.Venta"
        mc:Ignorable="d"
        Title="Meses Generados" Height="400" Width="300" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label x:Name="lblNumeroDeContrato" Content="Contrato Número: XXXXX" Margin="10,0,0,333" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="272"/>
        <ListBox x:Name="lsbMesesGenerados" Height="293" Margin="0,0,10,35" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="272"/>
        <Button x:Name="btnSalir" Content="Continuar" HorizontalAlignment="Left" Margin="10,0,0,10" VerticalAlignment="Bottom" Width="75" Click="btnSalir_Click" BorderThickness="1"/>

    </Grid>
</Window>
