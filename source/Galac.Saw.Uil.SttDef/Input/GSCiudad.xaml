<UserControl
    x:ClassModifier="internal"
    x:Class="Galac.Saw.Uil.SttDef.Input.GSCiudad"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:nsWpfRd="clr-namespace:LibGalac.Aos.UI.WpfRD;assembly=LibGalac.Aos.UI.WpfRD"
    xmlns:nsWpfControls="clr-namespace:LibGalac.Aos.UI.WpfControls;assembly=LibGalac.Aos.UI.WpfControls">
 <GroupBox Header=" " Margin="3">   
    <Grid Name="gwMain">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto" />
            <ColumnDefinition Width="23*" />
        </Grid.ColumnDefinitions>
        <Label Style="{DynamicResource {x:Static nsWpfRd:rsdInputWin.LabelPrompt}}" Name="lblNombreCiudad" Margin="3">Nombre de la Ciudad</Label>
        <nsWpfControls:GSTextBoxWpf InputDataType="Alfanumeric" MaxLength="100" Name="txtNombreCiudad" BorderThickness="1" TabIndex="0" Grid.Column="1" Margin="3" Width="200" />
    </Grid>
</GroupBox>
</UserControl>
