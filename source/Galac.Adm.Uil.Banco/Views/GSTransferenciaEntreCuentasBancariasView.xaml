                            IsChecked="{Binding Path=GeneraIGTFComisionEgreso}"
                            IsEnabled="{Binding Path=IsEnabledGeneraIGTFComisionEgreso}"
                            Style="{StaticResource CheckBoxStyle}" />
                    </Grid>
                </GroupBox>
            </Grid>
        </GroupBox>

