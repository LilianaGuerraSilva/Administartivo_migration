    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:nsVm="clr-namespace:Galac.Adm.Uil.Banco.ViewModel"
    xmlns:nsWpf="clr-namespace:LibGalac.Aos.UI.Wpf;assembly=LibGalac.Aos.UI.Wpf"
    xmlns:nsWpfControls="clr-namespace:LibGalac.Aos.UI.WpfControls;assembly=LibGalac.Aos.UI.WpfControls">

    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LibGalac.Aos.UI.WpfRD;component/rsdInputControl.xaml" />
                <ResourceDictionary Source="/LibGalac.Aos.UI.WpfRD;component/rsdButton.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <BooleanToVisibilityConverter x:Key="booleanToVisibilityConverter" />
        </ResourceDictionary>
    </UserControl.Resources>

    <Grid FocusManager.FocusedElement="{Binding ElementName=dtpFecha}">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto" />
            <ColumnDefinition Width="Auto" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>

        <GroupBox
            Grid.Row="0"
            Grid.ColumnSpan="2"
            Margin="10,3"
            VerticalAlignment="Top"
            Header="Datos Generales">

            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="120" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>

                <Label
                    Grid.Row="0"
                    Grid.Column="0"
                    Content="Fecha"
                    Style="{StaticResource LabelPrompt}" />
                <nsWpfControls:GSDatePickerWpf
                    Name="dtpFecha"
                    Grid.Row="0"
                    Grid.Column="1"
                    Width="120"
                    IsEnabled="{Binding Path=IsInsertar}"
                    SelectedDate="{Binding Path=Fecha, NotifyOnValidationError=True, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}"
                    Style="{StaticResource DatePickerStyle}" />
                <Label
                    Name="lblConsecutivo"
                    Grid.Row="0"
                    Grid.Column="2"
                    Width="150"
                    HorizontalAlignment="Right"
                    HorizontalContentAlignment="Right"
                    Content="{Binding Path=Consecutivo}"
                    Style="{StaticResource LabelPrompt}"
                    Visibility="Hidden" />
                <Label
                    Grid.Row="1"
                    Grid.Column="0"
                    Content="Nº Documento"
                    Style="{StaticResource LabelPrompt}" />
                <nsWpfControls:GSTextBoxWpf
                    Name="txtNumeroDocumento"
                    Grid.Row="1"
                    Grid.Column="1"
                    Margin="3,1,0,5"
                    HorizontalAlignment="Stretch"
                    BorderThickness="1"
                    InputDataType="Alfanumeric"
                    IsEnabled="{Binding Path=IsInsertar}"
                    MaxLength="20"
                    Style="{StaticResource TextBoxInputStyle}"
                    Text="{Binding Path=NumeroDocumento, NotifyOnValidationError=True, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}"
                    Visibility="{Binding Path=IsVisibleEditarNumeroDocumento, Converter={StaticResource booleanToVisibilityConverter}}" />
                <nsWpfControls:GSSeekConnection
                    Name="txtNumero"
                    Grid.Row="1"
                    Grid.Column="1"
                    Margin="3,1,0,1"
                    BorderThickness="1"
                    ConnectionValue="{Binding Path=NumeroDocumento, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnDataErrors=True}"
                    DisplayMemberPath="Numero"
                    MaxLength="20"
                    Model="{Binding Path=ConexionNumeroTransferencia, Mode=TwoWay}"
                    SearchCommand="{Binding Path=ChooseNumeroTransferenciaCommand}"
                    Visibility="{Binding Path=IsVisibleEscogerNumeroTransferencia, Converter={StaticResource booleanToVisibilityConverter}}" />
                <Label
                    Name="lblStatus"
                    Grid.Row="1"
                    Grid.Column="2"
                    Width="300"
                    HorizontalAlignment="Right"
                    HorizontalContentAlignment="Right"
                    Content="{Binding Path=StatusYFechaDeAnulacion}"
                    Style="{StaticResource LabelStatusBlue}"
                    Visibility="{Binding Path=IsVisibleAlContabilizar, Converter={StaticResource booleanToVisibilityConverter}}" />
                <Label
                    Grid.Row="2"
                    Grid.Column="0"
                    Content="Descripción"
                    Style="{StaticResource LabelPrompt}" />
                <nsWpfControls:GSTextBoxWpf
                    Name="txtDescripcion"
                    Grid.Row="2"
                    Grid.Column="1"
                    Grid.ColumnSpan="2"
                    Width="650"
                    Height="50"
                    BorderThickness="1"
                    InputDataType="String"
                    IsEnabled="{Binding Path=IsInsertar}"
                    MaxLength="255"
                    Style="{StaticResource TextBoxInputStyle}"
                    Text="{Binding Path=Descripcion, NotifyOnValidationError=True, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}"
                    VerticalScrollBarVisibility="Auto" />
            </Grid>
        </GroupBox>

