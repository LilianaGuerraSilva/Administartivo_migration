<UserControl
    x:ClassModifier="internal"
    x:Class="Galac.Saw.Uil.Vehiculo.Input.GSModelo"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:nsWpfRd="clr-namespace:LibGalac.Aos.UI.WpfRD;assembly=LibGalac.Aos.UI.WpfRD"
    xmlns:nsWpfControls="clr-namespace:LibGalac.Aos.UI.WpfControls;assembly=LibGalac.Aos.UI.WpfControls">
    <Grid Name="gwMain">
        <Label Style="{DynamicResource {x:Static nsWpfRd:rsdInputWin.LabelPrompt}}" Name="lblNombre" Margin="10,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Top">Nombre</Label>
        <nsWpfControls:GSTextBoxWpf InputDataType="Alfanumeric" Margin="120,0,0,0" MaxLength="20" Name="txtNombre" VerticalAlignment="Top" BorderThickness="1" TabIndex="0" />
        <Label Style="{DynamicResource {x:Static nsWpfRd:rsdInputWin.LabelPrompt}}" Name="lblMarca" Margin="10,28,0,0" HorizontalAlignment="Left" VerticalAlignment="Top">Marca</Label>
        <nsWpfControls:GSTextBoxWpf InputDataType="String" Margin="120,28,0,0" MaxLength="20" Name="txtMarca" VerticalAlignment="Top" BorderThickness="1" TabIndex="1" InvalidCharacters="'¬|%()$#,;:.="/>
    </Grid>
</UserControl>
