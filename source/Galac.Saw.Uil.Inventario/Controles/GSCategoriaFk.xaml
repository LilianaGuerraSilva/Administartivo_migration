<UserControl
    x:Class="Galac.Saw.Uil.Inventario.Controles.GSCategoriaFk"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:nsWpfRd="clr-namespace:LibGalac.Aos.UI.WpfRD;assembly=LibGalac.Aos.UI.WpfRD"
    xmlns:nsWpfControls="clr-namespace:LibGalac.Aos.UI.WpfControls;assembly=LibGalac.Aos.UI.WpfControls">
    <Grid Name="gwMain">
        <Label Style="{DynamicResource {x:Static nsWpfRd:rsdInputWin.LabelPrompt}}" Name="lblDescripcion" HorizontalAlignment="Left" VerticalAlignment="Top">Categoría</Label>
        <nsWpfControls:GSTextBoxWpf AcceptsReturn="False" AcceptsTab="False" Margin="120,0,0,0" MaxLength="20" Name="txtDescripcion" BorderThickness="1" TabIndex="0"/>
    </Grid>
</UserControl>
